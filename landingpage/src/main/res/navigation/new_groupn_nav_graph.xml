<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/new_groupn_navigation"
    app:startDestination="@id/createGroupFragment">

    <fragment
        android:id="@+id/createGroupFragment"
        android:name="com.wayapay.ng.landingpage.group.CreateGroupFragment"
        android:label="CreateGroupFragment" >
        <action
            android:id="@+id/action_createGroupFragment_to_createGroupImageFragment"
            app:destination="@id/createGroupImageFragment"
            app:enterAnim="@anim/slide_in_right"
            app:exitAnim="@anim/fade_out"
            app:popEnterAnim="@anim/fade_in"
            app:popExitAnim="@anim/slide_out_right" />
    </fragment>
    <fragment
        android:id="@+id/createGroupImageFragment"
        android:name="com.wayapay.ng.landingpage.group.CreateGroupImageFragment"
        android:label="fragment_create_group_image"
        tools:layout="@layout/fragment_create_group_image" />
</navigation>